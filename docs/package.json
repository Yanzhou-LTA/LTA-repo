{
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "slides.md",
          "pages/*.md"
        ],
        "options": {
          "parser": "slidev",
          "plugins": [
            "prettier-plugin-slidev"
          ]
        }
      }
    ]
  }
}
